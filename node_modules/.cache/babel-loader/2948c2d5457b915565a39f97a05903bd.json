{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport HeaderNav from './Header.vue';\nimport axios from 'axios';\nexport default {\n  name: \"HomePage\",\n  components: {\n    HeaderNav\n  },\n  computed: mapGetters([\"allnames\"]),\n\n  data() {\n    return {\n      UserName: \"\",\n      restaurants: []\n    };\n  },\n\n  methods: {\n    async deleteRes(id) {\n      let result = await axios.delete('http://localhost:3000/restaurants/' + id);\n\n      if (result.status == 200) {\n        this.loadData();\n      }\n    },\n\n    async loadData() {\n      const user = localStorage.getItem('user - info');\n      this.UserName = JSON.parse(user).name;\n      const result = await axios.get('http://localhost:3000/restaurants');\n      this.restaurants = result.data;\n    }\n\n  },\n\n  mounted() {\n    const user = localStorage.getItem('user - info');\n\n    if (!user) {\n      this.$router.push({\n        name: 'SignUp'\n      });\n    }\n\n    this.loadData();\n  }\n\n};","map":{"version":3,"mappings":"AAyDA,SAASA,UAAT,QAA2B,MAA3B;AACA,OAAOC,SAAP,MAAsB,cAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,UADK;EAEXC,UAAU,EAAE;IACRH;EADQ,CAFD;EAMXI,QAAQ,EAAEL,UAAU,CAAC,CAAC,UAAD,CAAD,CANT;;EAOXM,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,WAAW,EAAE;IAFV,CAAP;EAIH,CAZU;;EAaXC,OAAO,EAAE;IACL,MAAMC,SAAN,CAAgBC,EAAhB,EAAoB;MAEhB,IAAIC,MAAK,GAAI,MAAMV,KAAK,CAACW,MAAN,CAAa,uCAAuCF,EAApD,CAAnB;;MACA,IAAIC,MAAM,CAACE,MAAP,IAAiB,GAArB,EAA0B;QACtB,KAAKC,QAAL;MACJ;IAEH,CARI;;IASL,MAAMA,QAAN,GAAiB;MACb,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,aAArB,CAAb;MACA,KAAKX,QAAL,GAAgBY,IAAI,CAACC,KAAL,CAAWJ,IAAX,EAAiBb,IAAjC;MACA,MAAMS,MAAK,GAAI,MAAMV,KAAK,CAACmB,GAAN,CAAU,mCAAV,CAArB;MACA,KAAKb,WAAL,GAAmBI,MAAM,CAACN,IAA1B;IACJ;;EAdK,CAbE;;EA6BXgB,OAAO,GAAG;IAEN,MAAMN,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,aAArB,CAAb;;IAEA,IAAI,CAACF,IAAL,EAAW;MAEP,KAAKO,OAAL,CAAaC,IAAb,CAAkB;QAAErB,IAAI,EAAE;MAAR,CAAlB;IACJ;;IAEA,KAAKY,QAAL;EACJ;;AAvCW,CAAf","names":["mapGetters","HeaderNav","axios","name","components","computed","data","UserName","restaurants","methods","deleteRes","id","result","delete","status","loadData","user","localStorage","getItem","JSON","parse","get","mounted","$router","push"],"sourceRoot":"","sources":["/home/ts/Desktop/pract_vue/Restaurants/restro_project/src/components/Home.vue"],"sourcesContent":["<template >\n    <div class=\"row\">\n        <HeaderNav />\n        <h2>welcome <span class=\"su\">{{ UserName }}</span></h2>\n        <div class=\"col\">\n            <table class=\"table\">\n                <thead>\n                    <tr>\n                        <th scope=\"col\">ID</th>\n                        <th scope=\"col\">R_Name</th>\n                        <th scope=\"col\">Address</th>\n                        <th scope=\"col\">Contact</th>\n                        <th scope=\"col\">Actions</th>\n                        <th scope=\"col\"></th>\n                    </tr>\n                </thead>\n                <tbody>\n                    <tr v-for=\"item in restaurants\" :key=\"item.id\">\n                        <th>{{ item.id }}</th>\n                        <td>{{ item.name }}</td>\n                        <td>{{ item.address }}</td>\n                        <td>{{ item.contact }}</td>\n                        <td>\n                            <router-link :to=\"'/updaterestro/' + item.id\" class=\"text-decoration-none\">Update\n                            </router-link>\n                            <button class=\"btn btn-danger mx-3\" v-on:click=\"deleteRes(item.id)\">Delete</button>\n                        </td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n    </div>\n    <h3 class=\"text-success \">Practice for Vuex</h3>\n    <div class=\"row \">\n        <div class=\"col \">\n            <table class=\"table\">\n                <thead>\n                    <tr>\n                        <th scope=\"col\">ID</th>\n                        <th scope=\"col\">Name</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    <tr v-for=\"(names, index) in allnames\" :key=\"index\">\n                        <th>{{ names.id }}</th>\n                        <td>{{ names.name }} </td>\n                        <td>\n                            <button class=\"btn btn-danger mx-3\" v-on:click=\"deleteRes(item.id)\">Delete</button>\n                        </td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex';\nimport HeaderNav from './Header.vue'\nimport axios from 'axios'\nexport default {\n    name: \"HomePage\",\n    components: {\n        HeaderNav\n    },\n\n    computed: mapGetters([\"allnames\"]),\n    data() {\n        return {\n            UserName: \"\",\n            restaurants: [],\n        }\n    },\n    methods: {\n        async deleteRes(id) {\n\n            let result = await axios.delete('http://localhost:3000/restaurants/' + id);\n            if (result.status == 200) {\n                this.loadData()\n            }\n\n        },\n        async loadData() {\n            const user = localStorage.getItem('user - info')\n            this.UserName = JSON.parse(user).name\n            const result = await axios.get('http://localhost:3000/restaurants');\n            this.restaurants = result.data\n        }\n    },\n    mounted() {\n\n        const user = localStorage.getItem('user - info')\n\n        if (!user) {\n\n            this.$router.push({ name: 'SignUp' })\n        }\n\n        this.loadData()\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}