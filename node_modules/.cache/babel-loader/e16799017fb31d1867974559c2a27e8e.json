{"ast":null,"code":"import HeaderNav from './Header.vue';\nimport axios from 'axios';\nexport default {\n  name: \"HomePage\",\n  components: {\n    HeaderNav\n  },\n\n  data() {\n    return {\n      UserName: \"\",\n      restaurants: []\n    };\n  },\n\n  methods: {\n    async deleteRes(id) {\n      let result = await axios.delete('http://localhost:3000/restaurants/' + id);\n\n      if (result.status == 200) {\n        this.loadData();\n      }\n    },\n\n    async loadData() {\n      const user = localStorage.getItem('user-info');\n      this.UserName = JSON.parse(user).name;\n      let result = await axios.get('http://localhost:3000/restaurants');\n      this.restaurants = result.data; // console.warn(this.restaurants)\n\n      alert(\"home\");\n\n      if (!user) {\n        alert(\"signup\");\n        this.$router.push({\n          name: 'SignUp'\n        });\n      }\n    }\n\n  },\n\n  mounted() {\n    this.loadData();\n  }\n\n};","map":{"version":3,"mappings":"AAiCA,OAAOA,SAAP,MAAsB,cAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,UADK;EAEXC,UAAU,EAAE;IACRH;EADQ,CAFD;;EAKXI,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,WAAW,EAAE;IAFV,CAAP;EAIH,CAVU;;EAWXC,OAAO,EAAE;IACL,MAAMC,SAAN,CAAgBC,EAAhB,EAAoB;MAEhB,IAAIC,MAAK,GAAI,MAAMT,KAAK,CAACU,MAAN,CAAa,uCAAuCF,EAApD,CAAnB;;MACA,IAAIC,MAAM,CAACE,MAAP,IAAiB,GAArB,EAA0B;QACtB,KAAKC,QAAL;MACJ;IAEH,CARI;;IASL,MAAMA,QAAN,GAAiB;MAEb,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,WAArB,CAAb;MACA,KAAKX,QAAL,GAAgBY,IAAI,CAACC,KAAL,CAAWJ,IAAX,EAAiBZ,IAAjC;MACA,IAAIQ,MAAK,GAAI,MAAMT,KAAK,CAACkB,GAAN,CAAU,mCAAV,CAAnB;MACA,KAAKb,WAAL,GAAmBI,MAAM,CAACN,IAA1B,CALa,CAMb;;MACAgB,KAAK,CAAC,MAAD,CAAL;;MACA,IAAI,CAACN,IAAL,EAAW;QACXM,KAAK,CAAC,QAAD,CAAL;QACI,KAAKC,OAAL,CAAaC,IAAb,CAAkB;UAAEpB,IAAI,EAAE;QAAR,CAAlB;MACJ;IACJ;;EArBK,CAXE;;EAkCXqB,OAAO,GAAG;IAEN,KAAKV,QAAL;EACJ;;AArCW,CAAf","names":["HeaderNav","axios","name","components","data","UserName","restaurants","methods","deleteRes","id","result","delete","status","loadData"