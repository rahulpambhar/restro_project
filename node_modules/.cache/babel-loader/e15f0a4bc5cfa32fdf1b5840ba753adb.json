{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport HeaderNav from './Header.vue';\nimport axios from 'axios';\nexport default {\n  name: \"HomePage\",\n  components: {\n    HeaderNav\n  },\n\n  data() {\n    return {\n      UserName: \"\",\n      restaurants: []\n    };\n  },\n\n  methods: {\n    async deleteRes(id) {\n      let result = await axios.delete('http://localhost:3000/restaurants/' + id);\n\n      if (result.status == 200) {\n        this.loadData();\n      }\n    },\n\n    async loadData() {\n      const user = localStorage.getItem('user - info');\n      this.UserName = JSON.parse(user).name;\n      const result = await axios.get('http://localhost:3000/restaurants');\n      this.restaurants = result.data;\n    }\n\n  },\n\n  mounted() {\n    const user = localStorage.getItem('user - info');\n\n    if (!user) {\n      this.$router.push({\n        name: 'SignUp'\n      });\n    }\n\n    this.loadData();\n  },\n\n  computed: mapGetters(\"allproducts\")\n};","map":{"version":3,"mappings":"AA8CA,SAASA,UAAT,QAA2B,MAA3B;AACA,OAAOC,SAAP,MAAsB,cAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,UADK;EAEXC,UAAU,EAAE;IACRH;EADQ,CAFD;;EAKXI,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,WAAW,EAAE;IAFV,CAAP;EAIH,CAVU;;EAWXC,OAAO,EAAE;IACL,MAAMC,SAAN,CAAgBC,EAAhB,EAAoB;MAEhB,IAAIC,MAAK,GAAI,MAAMT,KAAK,CAACU,MAAN,CAAa,uCAAuCF,EAApD,CAAnB;;MACA,IAAIC,MAAM,CAACE,MAAP,IAAiB,GAArB,EAA0B;QACtB,KAAKC,QAAL;MACJ;IAEH,CARI;;IASL,MAAMA,QAAN,GAAiB;MACb,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,aAArB,CAAb;MACA,KAAKX,QAAL,GAAgBY,IAAI,CAACC,KAAL,CAAWJ,IAAX,EAAiBZ,IAAjC;MACA,MAAMQ,MAAK,GAAI,MAAMT,KAAK,CAACkB,GAAN,CAAU,mCAAV,CAArB;MACA,KAAKb,WAAL,GAAmBI,MAAM,CAACN,IAA1B;IACJ;;EAdK,CAXE;;EA2BXgB,OAAO,GAAG;IAEN,MAAMN,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,aAArB,CAAb;;IAEA,IAAI,CAACF,IAAL,EAAW;MAEP,KAAKO,OAAL,CAAaC,IAAb,CAAkB;QAAEpB,IAAI,EAAE;MAAR,CAAlB;IACJ;;IAEA,KAAKW,QAAL;EACH,CArCU;;EAsCXU,QAAQ,EACJxB,UAAU,CAAC,aAAD;AAvCH,CAAf","names":["mapGetters","HeaderNav","axios","name","components","data","UserName","restaurants","methods","deleteRes","id","result","delete","status","loadData","user","localStorage","getItem","JSON","parse","get","mounted","$router","push","computed"],"sourceRoot":"","sources":["/home/ts/Desktop/pract_vue/Restaurants/restro_project/src/components/Home.vue"],"sourcesContent":["<template >\n    <div class=\"row\">\n        <HeaderNav />\n        <h2>welcome {{ UserName }}</h2>\n\n        <table class=\"table\">\n            <thead>\n                <tr>\n                    <th scope=\"col\">ID</th>\n                    <th scope=\"col\">R_Name</th>\n                    <th scope=\"col\">Address</th>\n                    <th scope=\"col\">Contact</th>\n                    <th scope=\"col\">Actions</th>\n                    <th scope=\"col\"></th>\n                </tr>\n            </thead>\n            <tbody>\n                <tr v-for=\"item in restaurants\" :key=\"item.id\">\n                    <th>{{ item.id }}</th>\n                    <td>{{ item.name }}</td>\n                    <td>{{ item.address }}</td>\n                    <td>{{ item.contact }}</td>\n                    <td>\n                        <router-link :to=\"'/updaterestro/' + item.id\" class=\"text-decoration-none\">Update</router-link>\n                        <button class=\"btn btn-danger mx-3\" v-on:click=\"deleteRes(item.id)\">Delete</button>\n                    </td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n    <div class=\"row  \">\n        <div class=\"col \">\n            <ul>\n                <li v-for=\"product in allproducts\" :key=\"product.id\">\n\n                    {{ product.title}}\n\n                </li>\n\n            </ul>\n\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex';\nimport HeaderNav from './Header.vue'\nimport axios from 'axios'\nexport default {\n    name: \"HomePage\",\n    components: {\n        HeaderNav\n    },\n    data() {\n        return {\n            UserName: \"\",\n            restaurants: [],\n        }\n    },\n    methods: {\n        async deleteRes(id) {\n\n            let result = await axios.delete('http://localhost:3000/restaurants/' + id);\n            if (result.status == 200) {\n                this.loadData()\n            }\n\n        },\n        async loadData() {\n            const user = localStorage.getItem('user - info')\n            this.UserName = JSON.parse(user).name\n            const result = await axios.get('http://localhost:3000/restaurants');\n            this.restaurants = result.data\n        }\n    },\n    mounted() {\n\n        const user = localStorage.getItem('user - info')\n\n        if (!user) {\n\n            this.$router.push({ name: 'SignUp' })\n        }\n\n        this.loadData()\n    },\n    computed:\n        mapGetters(\"allproducts\")\n\n\n\n\n}\n</script>"]},"metadata":{},"sourceType":"module"}