{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport HeaderNav from './Header.vue';\nimport axios from 'axios';\nexport default {\n  name: \"HomePage\",\n  components: {\n    HeaderNav\n  },\n  computed: mapGetters([\"allnames\"]),\n\n  data() {\n    return {\n      UserName: \"\",\n      restaurants: [],\n      name: \"\"\n    };\n  },\n\n  methods: {\n    async deleteRes(id) {\n      let result = await axios.delete('http://localhost:3000/restaurants/' + id);\n\n      if (result.status == 200) {\n        this.loadData();\n      }\n    },\n\n    async loadData() {\n      const user = localStorage.getItem('user - info');\n      this.UserName = JSON.parse(user).name;\n      const result = await axios.get('http://localhost:3000/restaurants');\n      this.restaurants = result.data;\n    },\n\n    ...mapActions(['addName', 'removeName']),\n\n    submitform() {\n      this.addName({\n        id: new Date(),\n        name: this.name\n      });\n      this.name = \"\";\n    }\n\n  },\n\n  mounted() {\n    const user = localStorage.getItem('user - info');\n\n    if (!user) {\n      this.$router.push({\n        name: 'SignUp'\n      });\n    }\n\n    this.loadData();\n  }\n\n};","map":{"version":3,"mappings":"AAqEA,SAASA,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,OAAOC,SAAP,MAAsB,cAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,UADK;EAEXC,UAAU,EAAE;IACRH;EADQ,CAFD;EAMXI,QAAQ,EAAEN,UAAU,CAAC,CAAC,UAAD,CAAD,CANT;;EAOXO,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,WAAW,EAAE,EAFV;MAGHL,IAAI,EAAE;IAHH,CAAP;EAKH,CAbU;;EAcXM,OAAO,EAAE;IACL,MAAMC,SAAN,CAAgBC,EAAhB,EAAoB;MAEhB,IAAIC,MAAK,GAAI,MAAMV,KAAK,CAACW,MAAN,CAAa,uCAAuCF,EAApD,CAAnB;;MACA,IAAIC,MAAM,CAACE,MAAP,IAAiB,GAArB,EAA0B;QACtB,KAAKC,QAAL;MACJ;IAEH,CARI;;IASL,MAAMA,QAAN,GAAiB;MACb,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,aAArB,CAAb;MACA,KAAKX,QAAL,GAAgBY,IAAI,CAACC,KAAL,CAAWJ,IAAX,EAAiBb,IAAjC;MACA,MAAMS,MAAK,GAAI,MAAMV,KAAK,CAACmB,GAAN,CAAU,mCAAV,CAArB;MACA,KAAKb,WAAL,GAAmBI,MAAM,CAACN,IAA1B;IACH,CAdI;;IAeL,GAAGN,UAAU,CAAC,CAAC,SAAD,EAAY,YAAZ,CAAD,CAfR;;IAgBLsB,UAAU,GAAG;MACT,KAAKC,OAAL,CAAa;QAAEZ,EAAE,EAAE,IAAIa,IAAJ,EAAN;QAAkBrB,IAAI,EAAE,KAAKA;MAA7B,CAAb;MACA,KAAKA,IAAL,GAAY,EAAZ;IACJ;;EAnBK,CAdE;;EAoCXsB,OAAO,GAAG;IAEN,MAAMT,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,aAArB,CAAb;;IAEA,IAAI,CAACF,IAAL,EAAW;MAEP,KAAKU,OAAL,CAAaC,IAAb,CAAkB;QAAExB,IAAI,EAAE;MAAR,CAAlB;IACJ;;IAEA,KAAKY,QAAL;EACJ;;AA9CW,CAAf","names":["mapGetters","mapActions","HeaderNav","axios","name","components","computed","data","UserName","restaurants","methods","deleteRes","id","result","delete","status","loadData","user","localStorage","getItem","JSON","parse","get","submitform","addName","Date","mounted","$router","push"],"sourceRoot":"","sources":["/home/ts/Desktop/pract_vue/Restaurants/restro_project/src/components/Home.vue"],"sourcesContent":["<template >\n    <div class=\"row\">\n        <HeaderNav />\n        <h2>welcome <span class=\"text-success \">{{ UserName }}</span></h2>\n        <div class=\"col\">\n            <table class=\"table\">\n                <thead>\n                    <tr>\n                        <th scope=\"col\">ID</th>\n                        <th scope=\"col\">R_Name</th>\n                        <th scope=\"col\">Address</th>\n                        <th scope=\"col\">Contact</th>\n                        <th scope=\"col\">Actions</th>\n                        <th scope=\"col\"></th>\n                    </tr>\n                </thead>\n                <tbody>\n                    <tr v-for=\"item in restaurants\" :key=\"item.id\">\n                        <th>{{ item.id }}</th>\n                        <td>{{ item.name }}</td>\n                        <td>{{ item.address }}</td>\n                        <td>{{ item.contact }}</td>\n                        <td>\n                            <router-link :to=\"'/updaterestro/' + item.id\" class=\"text-decoration-none\">Update\n                            </router-link>\n                            <button class=\"btn btn-danger mx-3\" v-on:click=\"deleteRes(item.id)\">Delete</button>\n                        </td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n    </div>\n    <h3 class=\"text-success \">Practice for Vuex</h3>\n    <div class=\"row \">\n        <div class=\"col-3 \">\n            <form @submit.prevent=\"submitform\">\n                <input type=\"text\" name=\"\" placeholder=\"Enter Name\" v-model=\"name\">\n                <input type=\"submit\" value=\"Add Name\" class=\"btn btn-primary mt-2\">\n            </form>\n            <table class=\"table\">\n                <thead>\n                    <tr>\n                        <!-- <th scope=\"col\">ID</th> -->\n                        <th scope=\"col\">Name</th>\n                        <th scope=\"col\" class=\"px-\">Actions</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    <tr v-for=\"(names, index) in allnames\" :key=\"index\">\n                        <!-- <th>{{ names.id }}</th> -->\n                        <td>{{ names.name }} </td>\n                        <td><button>\n                                <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"25\" height=\"25\" fill=\"currentColor\"\n                                    class=\"bi bi-cart-x\" viewBox=\"0 0 16 16\">\n                                    <path\n                                        d=\"M7.354 5.646a.5.5 0 1 0-.708.708L7.793 7.5 6.646 8.646a.5.5 0 1 0 .708.708L8.5 8.207l1.146 1.147a.5.5 0 0 0 .708-.708L9.207 7.5l1.147-1.146a.5.5 0 0 0-.708-.708L8.5 6.793 7.354 5.646z\" />\n                                    <path\n                                        d=\"M.5 1a.5.5 0 0 0 0 1h1.11l.401 1.607 1.498 7.985A.5.5 0 0 0 4 12h1a2 2 0 1 0 0 4 2 2 0 0 0 0-4h7a2 2 0 1 0 0 4 2 2 0 0 0 0-4h1a.5.5 0 0 0 .491-.408l1.5-8A.5.5 0 0 0 14.5 3H2.89l-.405-1.621A.5.5 0 0 0 2 1H.5zm3.915 10L3.102 4h10.796l-1.313 7h-8.17zM6 14a1 1 0 1 1-2 0 1 1 0 0 1 2 0zm7 0a1 1 0 1 1-2 0 1 1 0 0 1 2 0z\" />\n                                </svg></button>\n                            <!-- <button class=\"btn btn-danger mx-3\" v-on:click=\"deleteRes(item.id)\">Delete</button> -->\n                        </td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from 'vuex';\nimport HeaderNav from './Header.vue'\nimport axios from 'axios'\nexport default {\n    name: \"HomePage\",\n    components: {\n        HeaderNav\n    },\n\n    computed: mapGetters([\"allnames\"]),\n    data() {\n        return {\n            UserName: \"\",\n            restaurants: [],\n            name: \"\",\n        }\n    },\n    methods: {\n        async deleteRes(id) {\n\n            let result = await axios.delete('http://localhost:3000/restaurants/' + id);\n            if (result.status == 200) {\n                this.loadData()\n            }\n\n        },\n        async loadData() {\n            const user = localStorage.getItem('user - info')\n            this.UserName = JSON.parse(user).name\n            const result = await axios.get('http://localhost:3000/restaurants');\n            this.restaurants = result.data\n        },\n        ...mapActions(['addName', 'removeName']),\n        submitform() {\n            this.addName({ id: new Date(), name: this.name });\n            this.name = \"\"\n        }\n\n    },\n    mounted() {\n\n        const user = localStorage.getItem('user - info')\n\n        if (!user) {\n\n            this.$router.push({ name: 'SignUp' })\n        }\n\n        this.loadData()\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}