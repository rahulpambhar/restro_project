://localhost:3000/restaurants');\n            // this.restaurants = result.data\n\n            if (!user) {\n                alert(\"signup\")\n                this.$router.push({ name: 'SignUp' })\n            }\n        }\n    },\n    mounted() {\n        alert(\"home mounted\")\n        this.loadData();\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}