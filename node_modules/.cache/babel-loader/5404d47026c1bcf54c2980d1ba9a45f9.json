{"ast":null,"code":"import useVuelidate from '@vuelidate/core';\nimport { required, email } from '@vuelidate/validators';\nimport { mapGetters, mapActions } from 'vuex';\nimport HeaderNav from './Header.vue';\nimport FilterToDo from './FilterToDo.vue';\nexport default {\n  name: \"ToDo\",\n  components: {\n    HeaderNav,\n    FilterToDo\n  },\n  computed: mapGetters(['alltodo']),\n\n  created() {\n    this.fetchUser();\n  },\n\n  data() {\n    return {\n      NewName: \"\",\n      NewEmail: '',\n      ToDoId: '',\n      ToDoName: \"\",\n      ToDoEmail: \"\",\n      R_Id: \"\",\n      R_name: '',\n      ErrName: \"\",\n      ErrEmail: \"\"\n    };\n  },\n\n  setup() {\n    return {\n      v$: useVuelidate()\n    };\n  },\n\n  methods: { ...mapActions(['fetchUser', 'RemoveId', '', 'addNewName']),\n\n    EditTodo(todo) {\n      this.ToDoName = todo.name;\n      this.ToDoEmail = todo.email;\n      this.ToDoId = todo.id;\n    },\n\n    updateToDo() {// if (this.ToDoName && this.ToDoEmail) {                \n      //     this.ErrNull()                \n      // }       \n      // if (!this.ToDoName) {\n      //     this.ErrName=\"Name Required\"\n      // }\n      // if (!this.ToDoEmail) {\n      //     this.ErrEmail = \"Email Required\"\n      // }\n    },\n\n    validations() {\n      return {\n        firsToDoNametName: {\n          required\n        },\n        // Matches this.firstName\n        lastName: {\n          required\n        },\n        // Matches this.lastName\n        contact: {\n          email: {\n            required,\n            email\n          } // Matches this.contact.email\n\n        }\n      };\n    },\n\n    ErrNull() {\n      this.ErrName = \"\";\n      this.ErrEmail = \"\";\n    },\n\n    NewData(data) {\n      this.NewName = data.NewName;\n      this.NewEmail = data.NewEmail;\n      this.addNewName({\n        id: \"New Id\",\n        name: this.NewName,\n        email: this.NewEmail\n      });\n    },\n\n    removeToDo(data) {\n      this.R_Id = data.id;\n      this.R_name = data.name;\n    }\n\n  },\n\n  mounted() {\n    const user = localStorage.getItem('user - info');\n\n    if (!user) {\n      this.$router.push({\n        name: 'LogIn'\n      });\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAkIA,OAAOA,YAAP,MAAyB,iBAAzB;AACA,SAASC,QAAT,EAAmBC,KAAnB,QAAgC,uBAAhC;AAEA,SAASC,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,OAAOC,SAAP,MAAsB,cAAtB;AACA,OAAOC,UAAP,MAAuB,kBAAvB;AACA,eAAe;EACXC,IAAI,EAAE,MADK;EAEXC,UAAU,EAAE;IACRH,SADQ;IACGC;EADH,CAFD;EAMXG,QAAQ,EAAEN,UAAU,CAAC,CAAC,SAAD,CAAD,CANT;;EAOXO,OAAO,GAAG;IACN,KAAKC,SAAL;EACH,CATU;;EAUXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,EADN;MAEHC,QAAQ,EAAE,EAFP;MAGHC,MAAM,EAAE,EAHL;MAIHC,QAAQ,EAAE,EAJP;MAKHC,SAAS,EAAE,EALR;MAMHC,IAAI,EAAE,EANH;MAOHC,MAAM,EAAE,EAPL;MAQHC,OAAO,EAAE,EARN;MASHC,QAAQ,EAAE;IATP,CAAP;EAWH,CAtBU;;EAuBXC,KAAK,GAAG;IACJ,OAAO;MAAEC,EAAE,EAAEvB,YAAY;IAAlB,CAAP;EACH,CAzBU;;EA0BXwB,OAAO,EAAE,EACL,GAAGpB,UAAU,CAAC,CAAC,WAAD,EAAc,UAAd,EAA0B,EAA1B,EAA8B,YAA9B,CAAD,CADR;;IAELqB,QAAQ,CAACC,IAAD,EAAO;MACX,KAAKV,QAAL,GAAgBU,IAAI,CAACnB,IAArB;MACA,KAAKU,SAAL,GAAiBS,IAAI,CAACxB,KAAtB;MACA,KAAKa,MAAL,GAAcW,IAAI,CAACC,EAAnB;IACH,CANI;;IAOLC,UAAU,GAAG,CAGT;MACA;MACA;MAEA;MACA;MACA;MACA;MACA;MACA;IAEH,CArBI;;IAsBLC,WAAW,GAAG;MACV,OAAO;QACHC,iBAAiB,EAAE;UAAE7B;QAAF,CADhB;QAC8B;QACjC8B,QAAQ,EAAE;UAAE9B;QAAF,CAFP;QAEqB;QACxB+B,OAAO,EAAE;UACL9B,KAAK,EAAE;YAAED,QAAF;YAAYC;UAAZ,CADF,CACsB;;QADtB;MAHN,CAAP;IAOH,CA9BI;;IA+BL+B,OAAO,GAAG;MACN,KAAKb,OAAL,GAAe,EAAf;MACA,KAAKC,QAAL,GAAgB,EAAhB;IACH,CAlCI;;IAmCLa,OAAO,CAACtB,IAAD,EAAO;MACV,KAAKC,OAAL,GAAeD,IAAI,CAACC,OAApB;MACA,KAAKC,QAAL,GAAgBF,IAAI,CAACE,QAArB;MACA,KAAKqB,UAAL,CAAgB;QAAER,EAAE,EAAE,QAAN;QAAgBpB,IAAI,EAAE,KAAKM,OAA3B;QAAoCX,KAAK,EAAE,KAAKY;MAAhD,CAAhB;IACH,CAvCI;;IAwCLsB,UAAU,CAACxB,IAAD,EAAO;MACb,KAAKM,IAAL,GAAYN,IAAI,CAACe,EAAjB;MACA,KAAKR,MAAL,GAAcP,IAAI,CAACL,IAAnB;IACH;;EA3CI,CA1BE;;EAuEX8B,OAAO,GAAG;IAEN,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,aAArB,CAAb;;IACA,IAAI,CAACF,IAAL,EAAW;MACP,KAAKG,OAAL,CAAa