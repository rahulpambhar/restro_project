n    components: {\n        HeaderNav\n    },\n    data() {\n        return {\n            restaurant: {\n                RestaurantUpdate: {\n                    name: \"\",\n                    address:\"\",\n                    contact:\"\"\n                }\n            }\n            // restaurant: {\n            //     name: \"\",\n            //     address: \"\",\n            //     contact: \"\"\n            // }\n        }\n    },\n    async mounted() {\n\n        const user = localStorage.getItem('user - info')\n        if (!user) {\n\n            this.$router.push({ name: 'LogIn' })\n        }\n\n        const result = await axios.get(`http://127.0.0.1:8000/api/updateRestorent/${this.$route.params.id}`);\n        this.restaurant = result.data\n    \n\n    },\n    methods: {\n        async update_Restaurant() {\n\n            let result = await axios.put('http://localhost:3000/restaurants/' + this.$route.params.id, {\n                name: this.restaurant.name,\n                address: this.restaurant.address,\n                contact: this.restaurant.contact\n            });\n\n            if (result.status === 201) {\n\n                this.$router.push({ name: 'Home' })\n            }\n        }\n\n    }\n\n}\n\n</script>"]},"metadata":{},"sourceType":"module"}