{"ast":null,"code":"import axios from 'axios';\nimport HeaderNav from './Header.vue';\nexport default {\n  name: 'UpdateRestro',\n  components: {\n    HeaderNav\n  },\n\n  data() {\n    return {\n      restaurant: {\n        RestaurantUpdate: {\n          name: \"\",\n          address: \"\",\n          contact: \"\"\n        }\n      } // restaurant: {\n      //     name: \"\",\n      //     address: \"\",\n      //     contact: \"\"\n      // }\n\n    };\n  },\n\n  async mounted() {\n    const user = localStorage.getItem('user - info');\n\n    if (!user) {\n      this.$router.push({\n        name: 'LogIn'\n      });\n    }\n\n    const result = await axios.get(`http://127.0.0.1:8000/api/updateRestorent/${this.$route.params.id}`);\n    this.restaurant = result.data;\n  },\n\n  methods: {\n    async update_Restaurant() {\n      let result = await axios.put('http://localhost:3000/restaurants/' + this.$route.params.id, {\n        name: this.restaurant.name,\n        address: this.restaurant.address,\n        contact: this.restaurant.contact\n      });\n\n      if (result.status === 201) {\n        this.$router.push({\n          name: 'Home'\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,SAAP,MAAsB,cAAtB;AACA,eAAe;EACXC,IAAI,EAAE,cADK;EAEXC,UAAU,EAAE;IACRF;EADQ,CAFD;;EAKXG,IAAI,GAAG;IACH,OAAO;MACHC,UAAU,EAAE;QACRC,gBAAgB,EAAE;UACdJ,IAAI,EAAE,EADQ;UAEdK,OAAO,EAAC,EAFM;UAGdC,OAAO,EAAC;QAHM;MADV,CADT,CAQH;MACA;MACA;MACA;MACA;;IAZG,CAAP;EAcH,CApBU;;EAqBX,MAAMC,OAAN,GAAgB;IAEZ,MAAMC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,aAArB,CAAb;;IACA,IAAI,CAACF,IAAL,EAAW;MAEP,KAAKG,OAAL,CAAaC,IAAb,CAAkB;QAAEZ,IAAI,EAAE;MAAR,CAAlB;IACJ;;IAEA,MAAMa,MAAK,GAAI,MAAMf,KAAK,CAACgB,GAAN,CAAW,6CAA4C,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAAG,EAA7E,CAArB;IACA,KAAKd,UAAL,GAAkBU,MAAM,CAACX,IAAzB;EAGH,CAjCU;;EAkCXgB,OAAO,EAAE;IACL,MAAMC,iBAAN,GAA0B;MAEtB,IAAIN,MAAK,GAAI,MAAMf,KAAK,CAACsB,GAAN,CAAU,uCAAuC,KAAKL,MAAL,CAAYC,MAAZ,CAAmBC,EAApE,EAAwE;QACvF