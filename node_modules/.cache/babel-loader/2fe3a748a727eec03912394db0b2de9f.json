{"ast":null,"code":"import HeaderNav from './Header.vue';\nimport axios from 'axios';\nexport default {\n  name: \"HomePage\",\n  components: {\n    HeaderNav\n  },\n\n  data() {\n    return {\n      UserName: \"\",\n      restaurants: [],\n      name: \"\",\n      editUser: \"\",\n      updatedName: \"\",\n      editId: \"\",\n      count: \"\",\n      user: '',\n      deletId: '',\n      deleteRestaurant: \"\"\n    };\n  },\n\n  methods: {\n    deleteRes(data) {\n      this.deletId = data.item.id;\n      this.deleteRestaurant = data.item.name;\n    },\n\n    async DeleteRestroConfirm(id) {\n      let result = await axios.post('http://127.0.0.1:8000/api/deleteRestaurant/' + id);\n\n      if (result.status == 200) {\n        this.loadData();\n      }\n    },\n\n    async loadData() {\n      const user = localStorage.getItem('user - info');\n      this.UserName = JSON.parse(user).name;\n      const result = await axios.post('http://127.0.0.1:8000/api/rastaurantDataBase');\n      this.restaurants = result.data.RestaurantsData;\n    },\n\n    submitform() {\n      this.addName({\n        id: this.idd(),\n        name: this.name\n      });\n    },\n\n    EditName(data, id) {\n      this.editUser = data;\n      this.editId = id;\n    },\n\n    idd() {\n      return this.count = \"New Name\";\n    }\n\n  },\n\n  mounted() {\n    this.user = localStorage.getItem('user - info');\n\n    if (!this.user) {\n      this.$router.push({\n        name: 'SignUp'\n      });\n    }\n\n    this.loadData();\n  }\n\n};","map":{"version":3,"mappings":"AAoEA,OAAOA,SAAP,MAAsB,cAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,UADK;EAEXC,UAAU,EAAE;IACRH;EADQ,CAFD;;EAMXI,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,WAAW,EAAE,EAFV;MAGHJ,IAAI,EAAE,EAHH;MAIHK,QAAQ,EAAE,EAJP;MAKHC,WAAW,EAAE,EALV;MAMHC,MAAM,EAAE,EANL;MAOHC,KAAK,EAAE,EAPJ;MAQHC,IAAI,EAAE,EARH;MASHC,OAAO,EAAE,EATN;MAUHC,gBAAgB,EAAE;IAVf,CAAP;EAYH,CAnBU;;EAoBXC,OAAO,EAAE;IACLC,SAAS,CAACX,IAAD,EAAO;MACZ,KAAKQ,OAAL,GAAeR,IAA