{"ast":null,"code":"import axios from 'axios';\nimport { mapGetters, mapActions } from 'vuex';\nimport HeaderNav from './Header.vue';\nexport default {\n  name: \"ToDo\",\n  components: {\n    HeaderNav\n  },\n\n  data() {\n    return {\n      UserName: \"\",\n      UserId: \"\",\n      user: {\n        name: \"\"\n      }\n    };\n  },\n\n  computed: mapGetters(['allToDos']),\n\n  created() {\n    this.fetchUser();\n  },\n\n  methods: { ...mapActions(['fetchUser', '']),\n\n    async deletetodo(id) {\n      let result = await axios.delete('http://localhost:3000/user/' + id);\n\n      if (result.status == 200) {\n        this.fetchUser();\n      }\n    },\n\n    EditTodo(name, id) {\n      this.UserName = name;\n      this.UserId = id;\n    },\n\n    async updateUser() {\n      let result = await axios.put('http://localhost:3000/user/' + this.UserId, {\n        address: this.restaurant.address,\n        contact: this.restaurant.contact\n      });\n\n      if (result.status === 200) {\n        this.fetchUser();\n      }\n    }\n\n  },\n\n  mounted() {\n    const user = localStorage.getItem('user - info');\n\n    if (!user) {\n      this.$router.push({\n        name: 'LogIn'\n      });\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAqEA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,SAASC,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,OAAOC,SAAP,MAAsB,cAAtB;AACA,eAAe;EACXC,IAAI,EAAE,MADK;EAEXC,UAAU,EAAE;IACRF;EADQ,CAFD;;EAKXG,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,MAAM,EAAE,EAFL;MAGHC,IAAI,EAAE;QACFL,IAAI,EAAE;MADJ;IAHH,CAAP;EAQH,CAdU;;EAeXM,QAAQ,EAAET,UAAU,CAAC,CAAC,UAAD,CAAD,CAfT;;EAgBXU,OAAO,GAAG;IACN,KAAKC,SAAL;EACH,CAlBU;;EAmBXC,OAAO,EAAE,EACL,GAAGX,UAAU,CAAC,CAAC,WAAD,EAAc,EAAd,CAAD,CADR;;IAGL,MAAMY,UAAN,CAAiBC,EAAjB,EAAqB;MAEjB,IAAIC,MAAK,GAAI,MAAMhB,KAAK,CAACiB,MAAN,CAAa,gCAAgCF,EAA7C,CAAnB;;MACA,IAAIC,MAAM,CAACE,MAAP,IAAiB,GAArB,EAA0B;QACtB,KAAKN,SAAL;MACJ;IACH,CATI;;IAULO,QAAQ,CAACf,IAAD,EAAOW,EAAP,EAAW;MACf,KAAKR,QAAL,GAAgBH,IAAhB;MACA,KAAKI,MAAL,GAAcO,EAAd;IAEH,CAdI;;IAeL,MAA