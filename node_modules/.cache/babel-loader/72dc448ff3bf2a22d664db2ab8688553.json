{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport HeaderNav from './Header.vue';\nimport FilterToDo from './FilterToDo.vue';\nexport default {\n  name: \"ToDo\",\n  components: {\n    HeaderNav,\n    FilterToDo\n  },\n  computed: mapGetters(['alltodo']),\n\n  created() {\n    this.fetchUser();\n  },\n\n  data() {\n    return {\n      NewName: \"\",\n      NewEmail: '',\n      ToDoId: '',\n      ToDoName: \"\",\n      ToDoEmail: \"\"\n    };\n  },\n\n  methods: { ...mapActions(['fetchUser', 'removeToDo', 'updateToDo', 'addNewName']),\n\n    EditTodo(todo) {\n      this.ToDoName = todo.name;\n      this.ToDoEmail = todo.email;\n      this.ToDoId = todo.id;\n    },\n\n    NewData(data) {\n      this.NewName = data.NewName;\n      this.NewEmail = data.NewEmail;\n      this.addNewName({\n        id: \"New Id\",\n        name: this.NewName,\n        email: this.NewEmail\n      });\n    }\n\n  },\n\n  mounted() {\n    const user = localStorage.getItem('user - info');\n\n    if (!user) {\n      this.$router.push({\n        name: 'LogIn'\n      });\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAyIA,SAASA,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,OAAOC,SAAP,MAAsB,cAAtB;AACA,OAAOC,UAAP,MAAuB,kBAAvB;AACA,eAAe;EACXC,IAAI,EAAE,MADK;EAEXC,UAAU,EAAE;IACRH,SADQ;IACGC;EADH,CAFD;EAKXG,QAAQ,EAAEN,UAAU,CAAC,CAAC,SAAD,CAAD,CALT;;EAMXO,OAAO,GAAG;IACN,KAAKC,SAAL;EACH,CARU;;EASXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,EADN;MAEHC,QAAQ,EAAE,EAFP;MAGHC,MAAM,EAAE,EAHL;MAIHC,QAAQ,EAAE,EAJP;MAKHC,SAAS,EAAE;IALR,CAAP;EASH,CAnBU;;EAoBXC,OAAO,EAAE,EACL,GAAGd,UAAU,CAAC,CAAC,WAAD,EAAc,YAAd,EAA4B,YAA5B,EAA0C,YAA1C,CAAD,CADR;;IAELe,QAAQ,CAACC,IAAD,EAAO;MAEX,KAAKJ,QAAL,GAAgBI,IAAI,CAACb,IAArB;MACA,KAAKU,SAAL,GAAiBG,IAAI,CAACC,KAAtB;MACA,KAAKN,MAAL,GAAcK,IAAI,CAACE,EAAnB;IACH,CAPI;;IAQLC,OAAO,CAACX,IAAD,EAAO;MACV,KAAKC,OAAL,GAAeD,IAAI,CAACC,OAApB;MACA,KAAKC,QAAL,GAAgBF,IAAI,CAACE,QAArB;MACA,KAAKU,UAAL,CAAgB;QAAEF,EAAE,EAAE,QAAN;QAAgBf,IAAI,EAAE,KAAKM,OAA3B;QAAoCQ,KAAK,EAAE,KAAKP;MAAhD,