{"ast":null,"code":"// import { mapActions } from 'vuex'\nexport default {\n  props: ['totalRecords'],\n  name: \"FilterToDo\",\n  data: function () {\n    return {\n      page: 1,\n      limit: '',\n      perPage: ''\n    };\n  },\n\n  created() {\n    this.changePage(0);\n  },\n\n  computed: {\n    pages() {\n      const reminder = this.totalRecords % this.perPage;\n      console.log();\n\n      if (reminder > 0) {\n        return Math.floor(this.totalRecords / this.perPage) + 1;\n      } else {\n        return this.totalRecords / this.perPage;\n      }\n    }\n\n  },\n  methods: {\n    FilterToDo(e) {\n      this.perPage = parseInt(e.target.options[e.target.options.selectedIndex].innerText);\n    },\n\n    changePage(val) {\n      switch (val) {\n        case 0:\n          this.page = 1;\n          break;\n\n        case -1:\n          this.page = this.page > 1 ? this.page - 1 : this.page;\n          break;\n\n        case 1:\n          this.page = this.page < this.pages ? this.page + 1 : this.page;\n          break;\n\n        case this.pages:\n          this.page = this.pages;\n          break;\n      }\n\n      this.$emit('input', {\n        page: this.page,\n        perPage: this.perPage\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BA;AACA,eAAe;EACXA,KAAK,EAAE,CAAC,cAAD,CADI;EAEXC,IAAI,EAAE,YAFK;EAGXC,IAAI,EAAE,YAAY;IACd,OAAO;MACHC,IAAI,EAAE,CADH;MAEHC,KAAK,EAAE,EAFJ;MAGHC,OAAO,EAAE;IAHN,CAAP;EAMH,CAVU;;EAWXC,OAAO,GAAG;IACN,KAAKC,UAAL,CAAgB,CAAhB;EACH,CAbU;;EAcXC,QAAQ,EAAE;IACNC,KAAK,GAAG;MAEJ,MAAMC,QAAO,GAAI,KAAKC,YAAL,GAAoB,KAAKN,OAA1C;MACAO,OAAO,CAACC,GAAR;;MACA,IAAIH,QAAO,GAAI,CAAf,EAAkB;QACd,OAAOI,IAAI,CAACC,KAAL,CAAW,KAAKJ,YAAL,GAAoB,KAAKN,OAApC,IAA+C,CAAtD;MACJ,CAFA,MAEO;QAEH,OAAO,KAAKM,YAAL,GAAoB,KAAKN,OAAhC;MAEJ;IACJ;;EAZM,CAdC;EA4BXW,OAAO,EAAE;IAELC,UAAU,CAACC,CAAD,EAAI;MACV,KAAKb,OAAL,GAAec,QAAQ,CAACD,CAAC,CAACE,MAAF,CAASC,OAAT,CAAiBH,CAAC,CAACE,MAAF,CAASC,OAAT,CAAiBC,aAAlC,EAAiDC,SAAlD,CAAvB;IAEH,CALI;;IAMLhB,UAAU,CAACiB,GAAD,EAAM;MACZ,QAAQA,GAAR;QACI,KAAK,CAAL;UAAQ,KAAKrB,IAAL,GAAY,CAAZ;UAAe;;QACvB,KAAK,CAAC,CAAN;UAAS,KAAKA,IAAL,GAAY,KAAKA,IAAL,GAAY,CAAZ,GAAgB,KAAKA,IAAL,GAAY,CAA5B,GAAgC,KAAKA,IAAjD;UAAuD;;QAChE,KAAK,CAAL;UAAQ,KAAKA,IAAL,GAAY,KAAKA,IAAL,GAAY,KAAKM,KAAjB,GAAyB,KAAKN,IAAL,GAAY,CAArC,GAAyC,KAAKA,IAA1D;UAAgE;;QACxE,KAAK,KAAKM,KAAV;UAAiB,KAAKN,IAAL,GAAY,KAAKM,KAAjB;UAAwB;MAJ7C;;MAMA,KAAKgB,KAAL,CAAW,OAAX,EAAoB;QAAEtB,IAAI,EAAE,KAAKA,IAAb;QAAmBE,OAAO,EAAE,KAAKA;MAAjC,CAApB;IACJ;;EAdK;AA5BE,CAAf","names":["props","name","data","page","limit","perPage","created","changePage","computed","pages","reminder","totalRecords","console","log","Math","floor","methods","FilterToDo","e","parseInt","target","options","selectedIndex","innerText","val","$emit"],"sourceRoot":"","sources":["/home/ts/Desktop/pract_vue/Restaurants/restro_project/src/components/FilterToDo.vue"],"sourcesContent":["<template>\n    <div class=\"col\">\n        <h5 class=\"text-primary d-inline \">Filter ToDos:</h5>\n        <select class=\"mx-2\" @change=\"FilterToDo($event)\">\n            <option value=\"5\">5</option>\n            <option value=\"10\">10</option>\n            <option value=\"20\">20</option>\n            <option value=\"30\">30</option>\n            <option value=\"40\">40</option>\n        </select>\n    </div>\n    <div class=\"col\">\n        <section>\n            <p>\n\n                <i class=\"fa-solid fa-angles-left text-info cursor-pointer \" @click=\"changePage(0)\"></i> |\n                <i class=\"fa-solid fa-angle-left\" @click=\"changePage(-1)\"></i> |\n                <span class=\"text-success\">\n                    SHOWING {{ page }} OF {{ pages }}\n                </span>\n                | <i class=\"fa-solid fa-angle-right\" @click=\"changePage(1)\"></i>\n                | <i class=\"fa-solid fa-angles-right text-info\" @click=\"changePage(pages)\"></i>\n\n\n            </p>\n            <h3 class=\"text-primary\">Total records:{{ this.totalRecords }}</h3>\n        </section>\n    </div>\n</template>\n\n<script>\n// import { mapActions } from 'vuex'\nexport default {\n    props: ['totalRecords'],\n    name: \"FilterToDo\",\n    data: function () {\n        return {\n            page: 1,\n            limit: '',\n            perPage: ''\n            \n        }\n    },\n    created() {\n        this.changePage(0)\n    },\n    computed: {\n        pages() {\n            \n            const reminder = this.totalRecords % this.perPage            \n            console.log();\n            if (reminder > 0) {            \n                return Math.floor(this.totalRecords / this.perPage) + 1\n            } else {\n               \n                return this.totalRecords / this.perPage\n          \n            }\n        }\n    },\n    methods: {\n    \n        FilterToDo(e) {\n            this.perPage = parseInt(e.target.options[e.target.options.selectedIndex].innerText)  \n                  \n        },\n        changePage(val) {\n            switch (val) {\n                case 0: this.page = 1; break;\n                case -1: this.page = this.page > 1 ? this.page - 1 : this.page; break;\n                case 1: this.page = this.page < this.pages ? this.page + 1 : this.page; break;\n                case this.pages: this.page = this.pages; break;\n            }\n            this.$emit('input', { page: this.page, perPage: this.perPage })\n        }\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}