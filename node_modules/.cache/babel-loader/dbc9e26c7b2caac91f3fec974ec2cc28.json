uter.push({ name: 'LogIn' })\n        }        \n        // const data = this.$route.params.id;\n        const result = await axios.get(`http://127.0.0.1:8000/api/updateRestorent/`);\n        // this.restaurant = result.data\n        console.log(result.data);\n        // console.log(data);\n\n    },\n    methods: {\n        async update_Restaurant() {\n\n            let result = await axios.put('http://localhost:3000/restaurants/' + this.$route.params.id, {\n                name: this.restaurant.name,\n                address: this.restaurant.address,\n                contact: this.restaurant.contact\n            });\n\n            if (result.status === 201) {\n\n                this.$router.push({ name: 'Home' })\n            }\n        }\n\n    }\n\n}\n\n</script>"]},"metadata":{},"sourceType":"module"}