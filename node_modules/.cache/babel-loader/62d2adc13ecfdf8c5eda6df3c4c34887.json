erName = name\n            this.UserId = id\n            this.UserEmail = email\n        },\n        async updateUser() {\n\n            let result = await axios.put('http://localhost:3000/user/' + this.UserId, {\n                name: this.UserName,\n                email: this.UserEmail,                \n            });\n            if (result.status === 200) {\n                this.fetchUser()\n            }\n        }\n    },\n    mounted() {\n\n        const user = localStorage.getItem('user - info')\n        if (!user) {\n            this.$router.push({ name: 'LogIn' })\n        }\n    }\n}\n</script>   "]},"metadata":{},"sourceType":"module"}