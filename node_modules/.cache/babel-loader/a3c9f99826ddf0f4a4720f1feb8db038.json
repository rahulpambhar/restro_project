{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport HeaderNav from './Header.vue';\nimport axios from 'axios';\nexport default {\n  name: \"HomePage\",\n  components: {\n    HeaderNav\n  },\n  computed: mapGetters([\"allNames\"]),\n\n  data() {\n    return {\n      UserName: \"\",\n      restaurants: [],\n      name: \"\",\n      editUser: \"\",\n      updatedName: \"\",\n      editId: \"\",\n      count: \"\",\n      user: '',\n      deletId: '',\n      deleteRestaurant: \"\"\n    };\n  },\n\n  methods: {\n    async deleteRes(data) {\n      this.deletId = data.item.id;\n      this.deleteRestaurant = data.item.name;\n    },\n\n    DeleteRestroConfirm(id) {\n      alert(\"sdjkb\"); // let result = await axios.post('http://127.0.0.1:8000/api/deleteRestaurant/' + id);\n      // if (result.status == 200) {\n      //     this.loadData()\n      // }\n    },\n\n    async loadData() {\n      const user = localStorage.getItem('user - info');\n      this.UserName = JSON.parse(user).name;\n      const result = await axios.post('http://127.0.0.1:8000/api/rastaurantDataBase');\n      this.restaurants = result.data.RestaurantsData;\n    },\n\n    ...mapActions(['addName', 'removeName', 'updateStateUser']),\n\n    submitform() {\n      this.addName({\n        id: this.idd(),\n        name: this.name\n      });\n    },\n\n    EditName(data, id) {\n      this.editUser = data;\n      this.editId = id;\n    },\n\n    idd() {\n      return this.count = \"New Name\";\n    }\n\n  },\n\n  mounted() {\n    this.user = localStorage.getItem('user - info');\n\n    if (!this.user) {\n      this.$router.push({\n        name: 'SignUp'\n      });\n    }\n\n    this.loadData();\n  }\n\n};","map":{"version":3,"mappings":"AAkEA,SAASA,UAAT,EAAqBC,UAArB,QAAuC,MAAvC;AACA,OAAOC,SAAP,MAAsB,cAAtB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,UADK;EAEXC,UAAU,EAAE;IACRH;EADQ,CAFD;EAKXI,QAAQ,EAAEN,UAAU,CAAC,CAAC,UAAD,CAAD,CALT;;EAMXO,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EADP;MAE